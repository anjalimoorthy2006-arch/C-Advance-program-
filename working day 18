#include <stdio.h>

int main() {
    int day;

    // Input day number
    printf("Enter day number (1-7): ");
    scanf("%d", &day);

    // Check working day or weekend
    if (day >= 1 && day <= 5) {
        printf("It is a Working Day.");
    } else if (day == 6 || day == 7) {
        printf("It is a Weekend.");
    } else {
        printf("Invalid day number.");
    }

    return 0;
}