#include <stdio.h>

int main() {
    int num, i;
    long long fact = 1;

    // Input number
    printf("Enter a number: ");
    scanf("%d", &num);

    // Factorial calculation
    if (num < 0) {
        printf("Factorial of a negative number is not defined.");
    } else {
        for (i = 1; i <= num; i++) {
            fact = fact * i;
        }
        printf("Factorial of %d = %lld", num, fact);
    }

    return 0;
}