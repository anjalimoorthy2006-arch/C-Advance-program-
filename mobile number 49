#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main() {
    char mobile[20];
    int i, valid = 1;

    printf("Enter mobile number: ");
    scanf("%s", mobile);

    // Check length
    if (strlen(mobile) != 10) {
        valid = 0;
    }

    // Check starting digit
    if (mobile[0] < '6' || mobile[0] > '9') {
        valid = 0;
    }

    // Check all digits
    for (i = 0; mobile[i] != '\0'; i++) {
        if (!isdigit(mobile[i])) {
            valid = 0;
            break;
        }
    }

    if (valid)
        printf("Valid mobile number\n");
    else
        printf("Invalid mobile number\n");

    return 0;
}