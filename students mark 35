#include <stdio.h>

int main() {
    int n, i, j, temp;

    // Ask for number of students
    printf("Enter the number of students: ");
    scanf("%d", &n);

    int marks[n];

    // Input marks
    for(i = 0; i < n; i++) {
        printf("Enter marks for student %d: ", i + 1);
        scanf("%d", &marks[i]);
    }

    // Sort marks using Bubble Sort
    for(i = 0; i < n - 1; i++) {
        for(j = 0; j < n - i - 1; j++) {
            if(marks[j] > marks[j + 1]) {
                // Swap
                temp = marks[j];
                marks[j] = marks[j + 1];
                marks[j + 1] = temp;
            }
        }
    }

    // Display sorted marks
    printf("\nSorted marks in ascending order:\n");
    for(i = 0; i < n; i++) {
        printf("%d ", marks[i]);
    }
    printf("\n");

    return 0;
}